<Estrellas>:
    adaptive_height: True
    orientation: 'horizontal'
    spacing: 10

    MDIconButton:
        id: starone
        icon: "star-outline"
        icon_size: "30sp"
        theme_icon_color: "Custom"
        icon_color: rgba(app.purpura)
        on_release:
            self.icon = "star" \
            if self.icon == "star-outline" else "star-outline"

